    flat\r\n          solo-inverted\r\n          hide-details\r\n          prepend-inner-icon=\"mdi-magnify\"\r\n          label=\"Search\"\r\n        ></v-text-field>\r\n        <template v-if=\"$vuetify.breakpoint.mdAndUp\">\r\n          <v-spacer></v-spacer>\r\n          <v-select\r\n            v-model=\"sortBy\"\r\n            flat\r\n            solo-inverted\r\n            hide-details\r\n            :items=\"keys\"\r\n            prepend-inner-icon=\"mdi-magnify\"\r\n            label=\"Sort by\"\r\n          ></v-select>\r\n          <v-spacer></v-spacer>\r\n          <v-btn-toggle v-model=\"sortDesc\" mandatory>\r\n            <v-btn large depressed color=\"blue\" :value=\"false\">\r\n              <v-icon>mdi-arrow-up</v-icon>\r\n            </v-btn>\r\n            <v-btn large depressed color=\"blue\" :value=\"true\">\r\n              <v-icon>mdi-arrow-down</v-icon>\r\n            </v-btn>\r\n          </v-btn-toggle>\r\n        </template>\r\n      </v-toolbar>\r\n    </template>\r\n\r\n    <template>\r\n      <v-row>\r\n        <v-col\r\n          v-for=\"item in restaurants\"\r\n          :key=\"item.name\"\r\n          cols=\"12\"\r\n          sm=\"6\"\r\n          md=\"4\"\r\n          lg=\"3\"\r\n        >\r\n          <RestaurantPresentationCard :dish=\"item\" />\r\n        </v-col>\r\n      </v-row>\r\n    </template>\r\n\r\n    <template v-slot:footer>\r\n      <v-row class=\"mt-2\" align=\"center\" justify=\"center\">\r\n        <span class=\"grey--text\">Items per page</span>\r\n        <v-menu offset-y>\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-btn\r\n              dark\r\n              text\r\n              color=\"primary\"\r\n              class=\"ml-2\"\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            >\r\n              {{ itemsPerPage }}\r\n              <v-icon>mdi-chevron-down</v-icon>\r\n            </v-btn>\r\n          </template>\r\n          <v-list>\r\n            <v-list-item\r\n              v-for=\"(number, index) in itemsPerPageArray\"\r\n              :key=\"index\"\r\n              @click=\"updateItemsPerPage(number)\"\r\n            >\r\n              <v-list-item-title>{{ number }}</v-list-item-title>\r\n            </v-list-item>\r\n          </v-list>\r\n        </v-menu>\r\n\r\n        <v-spacer></v-spacer>\r\n\r\n        <span class=\"mr-4 grey--text\">\r\n          Page {{ page }} of {{ numberOfPages }}\r\n        </span>\r\n        <v-btn fab dark color=\"blue darken-3\" class=\"mr-1\" @click=\"formerPage\">\r\n          <v-icon>mdi-chevron-left</v-icon>\r\n        </v-btn>\r\n        <v-btn fab dark color=\"blue darken-3\" class=\"ml-1\" @click=\"nextPage\">\r\n          <v-icon>mdi-chevron-right</v-icon>\r\n        </v-btn>\r\n      </v-row>\r\n    </template>\r\n  </v-data-iterator>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RestaurantPresentationCard from \"@/components/restaurantPresentationCard.vue\";\r\nimport RestaurantService from \"@/store/restaurant.js\";\r\nexport default {\r\n  name: \"RestaurantPresentation\",\r\n  store: { RestaurantService },\r\n  components: { RestaurantPresentationCard },\r\n  data() {\r\n    return {\r\n      itemsPerPageArray: [4, 8, 12],\r\n      search: \"\",\r\n      filter: {},\r\n      sortDesc: false,\r\n      page: 1,\r\n      itemsPerPage: 4,\r\n      sortBy: \"name\",\r\n      keys: [\"Name\"],\r\n      items: [],\r\n    };\r\n  },\r\n  mounted: async function () {\r\n    RestaurantService.commit(\"getAllRestaurants\");\r\n  },\r\n  computed: {\r\n    numberOfPages() {\r\n      return Math.ceil(this.items.length / this.itemsPerPage);\r\n    },\r\n    filteredKeys() {\r\n      return this.keys.filter((key) => key !== \"Name\");\r\n    },\r\n    restaurants() {\r\n      return RestaurantService.state.restaurants;\r\n    },\r\n  },\r\n  methods: {\r\n    nextPage() {\r\n      if (this.page + 1 <= this.numberOfPages) this.page += 1;\r\n    },\r\n    formerPage() {\r\n      if (this.page - 1 >= 1) this.page -= 1;\r\n    },\r\n    updateItemsPerPage(number) {\r\n      this.itemsPerPage = number;\r\n    },\r\n  },\r\n};\r\n</script>"]}]}